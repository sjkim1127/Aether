{
    "name": "@freexxx9206/aether-codegen",
    "version": "0.1.4",
    "description": "AI-powered dynamic code injection framework - Generate code with AI using templates",
    "main": "index.js",
    "types": "index.d.ts",
    "author": "Aether Team",
    "license": "MIT",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sjkim1127/Aether"
    },
    "keywords": [
        "ai",
        "code-generation",
        "template",
        "openai",
        "gpt",
        "anthropic",
        "claude",
        "ollama",
        "llm",
        "codegen",
        "rust",
        "napi"
    ],
    "engines": {
        "node": ">= 18"
    },
    "napi": {
        "binaryName": "aether-codegen",
        "targets": [
            "x86_64-pc-windows-msvc"
        ]
    },
    "files": [
        "index.js",
        "index.d.ts",
        "*.node"
    ],
    "scripts": {
        "build": "napi build --platform --release",
        "build:debug": "napi build --platform",
        "test": "node test.js"
    },
    "devDependencies": {
        "@napi-rs/cli": "^3.5.1"
    }
}